<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TumiReport" parallel="none">


	<test name="GuestOrder">
		<classes>
			<class name="com.tumi.guestOrders.GuestOrder" >
			<methods><include name="tumiOrderAsGuest"></include>
			</methods>
			</class>
		</classes>
	</test>
	<test name="OrderWithGiftBox">
		<classes>
			<class name="com.tumi.guestOrders.OrderWithGiftBox" >
			<methods><include name="orderWithGiftBoxAsGuest"></include>
			</methods>
			</class>
		</classes>
	</test>
	<test name="OrderWithVochercode">
		<classes>
			<class name="com.tumi.guestOrders.OrderWithVochercode" >
			<methods><include name="verifyOrderWithVocherCode"></include>
			</methods>
			</class>
		</classes>
	</test>
		<test name="RegisteredOrder">
		
		<classes>
			<class name="com.tumi.registerOrders.RegisteredOrder" />
		</classes>
	</test>
	<test name="RegisterOrderWithVoucher">
		
		<classes>
			<class name="com.tumi.registerOrders.RegisterOrderWithVoucher" />
		</classes>
	</test>
	
	<test name="GoogleLogin">
		
		<classes>
			<class name="com.tumi.login.GoogleLogin" />
		</classes>
	</test>
	<test name="InstagramLogin">
		
		<classes>
			<class name="com.tumi.login.InstagramLogin" />
		</classes>
	</test>

	<test name="RegularLogin">
		
		<classes>
			<class name="com.tumi.login.RegularLogin" />
		</classes>
	</test>
	</suite>