<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Tests Suite">

	
	
	<test name="GuestOrders">
		<classes>
			<class name="com.tumi.guestOrders.GuestOrders">
			<methods>
				<include name="tumiOrderAsGuest"></include>
			</methods>
			</class>
		</classes>
	</test>
	
	<test name="GlobalLocatororder">
		<classes>
			<class name="com.tumi.guestOrders.GlobalLocatororder">
			</class>
		</classes>
	</test>
	
	<test name="OrderWithGiftBox">
		<classes>
			<class name="com.tumi.guestOrders.OrderWithGiftBox">
			<methods>
			<exclude name="backOrderAsGuest"></exclude>
			</methods>
			</class>
		</classes>
	</test>
	
	<test name="RegisteredOrder">
		<classes>
			<class name="com.tumi.registerOrders.RegisteredOrder">
			</class>
		</classes>
	</test>
	
	<test name="GuestOrderWithPaypal">
		<classes>
			<class name="com.tumi.guestOrders.GuestOrderWithPaypal">
			</class>
		</classes>
	</test>
	
	<test name="RegisteredOrderWithPayPal">
		<classes>
			<class name="com.tumi.registerOrders.RegisteredOrderWithPayPal">
			</class>
		</classes>
	</test>
	
	<test name="CreateAccount">
		<classes>
			<class name="com.tumi.userRegistration.CreateAccount">
			</class>
		</classes>
	</test>
	
</suite>
